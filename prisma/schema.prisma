generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model users {
  id         Int       @id @default(autoincrement())
  email      String    @unique
  password   String
  name       String?   
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
